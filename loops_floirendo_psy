dna_strand_1 = "EKNHQDMCN-CGG-GAC-CIG-GCC-GIC" # TAG-GCC-CTG-CIG-GAC-CGG-CAG

dna_strand_2 = ""

for x in range(len(dna_strand_1)):
    char = dna_strand_1[x]

    if char == "A":
        dna_strand_2 += "B"
    elif char == "B":
        dna_strand_2 += "C"
    elif char == "C":
        dna_strand_2 += "D"
    elif char == "D":
        dna_strand_2 += "E"
    elif char == "E":
        dna_strand_2 += "F"
    elif char == "F":
        dna_strand_2 += "G"
    elif char == "G":
        dna_strand_2 += "H"
    elif char == "H":
        dna_strand_2 += "I"
    elif char == "I":
        dna_strand_2 += "J"
    elif char == "J":
        dna_strand_2 += "K"
    elif char == "K":
        dna_strand_2 += "L"
    elif char == "L":
        dna_strand_2 += "M"
    elif char == "M":
        dna_strand_2 += "N"
    elif char == "N":
        dna_strand_2 += "O"
    elif char == "O":
        dna_strand_2 += "P"
    elif char == "P":
        dna_strand_2 += "Q"
    elif char == "Q":
        dna_strand_2 += "R"
    elif char == "R":
        dna_strand_2 += "S"
    elif char == "S":
        dna_strand_2 += "T"
    elif char == "T":
        dna_strand_2 += "U"
    elif char == "U":
        dna_strand_2 += "V"
    elif char == "V":
        dna_strand_2 += "W"
    elif char == "W":
        dna_strand_2 += "X"
    elif char == "X":
        dna_strand_2 += "Y"
    elif char == "Y":
        dna_strand_2 += "Z"

    else:
        dna_strand_2 += char

print(dna_strand_2)
